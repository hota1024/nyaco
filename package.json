{
  "name": "nyaco",
  "version": "0.0.0",
  "license": "MIT",
  "description": "üêæ text programming language that compiles to Scratch 3.0 project",
  "repository": {
    "type": "git",
    "url": "hota1024/nyaco"
  },
  "bin": {
    "nyaco": "bin/nyaco.js"
  },
  "type": "module",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/"
  ],
  "engines": {
    "vscode": "^1.22.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "nyaco",
        "extensions": [
          ".nya"
        ]
      }
    ]
  },
  "scripts": {
    "build": "rollup -c",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "run": "ts-node -r tsconfig-paths/register",
    "test": "jest",
    "lint": "eslint",
    "prepublish": "npm run build"
  },
  "devDependencies": {
    "@rollup/plugin-eslint": "^8.0.1",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.8.4",
    "rollup": "^2.37.1",
    "rollup-plugin-ts": "^3.2.0",
    "ts-jest": "^26.4.4",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.9.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/tests/**/*.test.ts"
    ],
    "moduleNameMapper": {
      "^@/(.+)": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "nyair": "^0.0.1",
    "rs-enums": "^0.0.0",
    "vscode": "^1.1.37",
    "vscode-languageclient": "^8.1.0"
  }
}
